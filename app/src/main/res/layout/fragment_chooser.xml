    <Button
        android:id="@+id/yunofishie"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/meme2"
        android:onClick="onClick"/>

    <Button
        android:id="@+id/buzzlightyear"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/meme3"
        android:onClick="onClick"/>

    <Button
        android:id="@+id/dwight"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/meme4"
        android:onClick="onClick"/>

